<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping xmlns:OpenClinica="http://www.openclinica.org/ns/odm_ext_v130/v3.1">

    <class name="core.org.akaza.openclinica.bean.submit.crfdata.ODMContainer">
        <map-to xml="ODM"/>

        <field name="CrfDataPostImportContainer"
               type="core.org.akaza.openclinica.bean.submit.crfdata.CRFDataPostImportContainer" required="true">
            <bind-xml name="ClinicalData"/>
        </field>
    </class>

    <class name="core.org.akaza.openclinica.bean.submit.crfdata.CRFDataPostImportContainer">
        <map-to xml="ClinicalData"/>

        <field name="SubjectData"
               type="core.org.akaza.openclinica.bean.submit.crfdata.SubjectDataBean"
               collection="arraylist">
            <bind-xml name="SubjectData"/>
        </field>

        <field name="UpsertOn"
               type="core.org.akaza.openclinica.bean.submit.crfdata.UpsertOnBean">
            <bind-xml name="UpsertOn"/>
        </field>

        <field name="StudyOID"
               type="java.lang.String">
            <bind-xml name="StudyOID" node="attribute"/>
        </field>
    </class>

    <class name="core.org.akaza.openclinica.bean.submit.crfdata.UpsertOnBean">
        <map-to xml="UpsertOn"/>

        <field name="NotStarted"
               type="java.lang.Boolean">
            <bind-xml name="NotStarted" node="attribute"/>
        </field>

        <field name="DataEntryStarted"
               type="java.lang.Boolean">
            <bind-xml name="DataEntryStarted" node="attribute"/>
        </field>

        <field name="DataEntryComplete"
               type="java.lang.Boolean">
            <bind-xml name="DataEntryComplete" node="attribute"/>
        </field>

    </class>
    <class name="core.org.akaza.openclinica.bean.submit.crfdata.SubjectDataBean">
        <map-to xml="SubjectData"/>

        <field name="SubjectOID"
               type="java.lang.String">
            <bind-xml name="SubjectKey" node="attribute"/>
        </field>
        <field name="StudySubjectID"
               type="java.lang.String">
            <bind-xml name="OpenClinica:StudySubjectID" node="attribute"/>
        </field>

        <field name="StudyEventData"
               type="core.org.akaza.openclinica.bean.submit.crfdata.StudyEventDataBean"
               collection="arraylist">
            <bind-xml name="StudyEventData"/>
        </field>
    </class>

    <class name="core.org.akaza.openclinica.bean.submit.crfdata.StudyEventDataBean">
        <map-to xml="StudyEventData"/>

        <field name="StudyEventOID"
               type="java.lang.String">
            <bind-xml name="StudyEventOID" node="attribute"/>
        </field>

        <field name="StudyEventRepeatKey"
               type="java.lang.String">
            <bind-xml name="StudyEventRepeatKey" node="attribute"/>
        </field>

        <field name="StartDate"
               type="java.lang.String">
            <bind-xml name="OpenClinica:StartDate" node="attribute"/>
        </field>

        <field name="EndDate"
               type="java.lang.String">
            <bind-xml name="OpenClinica:EndDate" node="attribute"/>
        </field>

        <field name="eventStatus"
               type="java.lang.String">
            <bind-xml name="OpenClinica:Status" node="attribute"/>
        </field>

        <field name="WorkflowStatusAsString"
               type="java.lang.String">
            <bind-xml name="OpenClinica:WorkflowStatus" node="attribute"/>
        </field>

        <field name="SignedString"
               type="java.lang.String">
            <bind-xml name="OpenClinica:Signed" node="attribute"/>
        </field>

        <field name="FormData"
               type="core.org.akaza.openclinica.bean.submit.crfdata.FormDataBean"
               collection="arraylist">
            <bind-xml name="FormData"/>
        </field>
        <field name="signatures"
               type="core.org.akaza.openclinica.bean.submit.crfdata.SignatureBean"
               collection="arraylist">
            <bind-xml name="Signature"/>
        </field>
    </class>
    <class name="core.org.akaza.openclinica.bean.submit.crfdata.SignatureBean">
        <map-to xml="Signature"/>
        <field name="attestation" type="java.lang.String">
            <bind-xml name="OpenClinica:Attestation" node="attribute"/>
        </field>
    </class>
    <class name="core.org.akaza.openclinica.bean.submit.crfdata.FormDataBean">
        <map-to xml="FormData"/>

        <field name="FormOID" type="java.lang.String">
            <bind-xml name="FormOID" node="attribute"/>
        </field>

        <field name="FormLayoutName" type="java.lang.String">
            <bind-xml name="OpenClinica:FormLayoutOID" node="attribute"/>
        </field>

        <field name="EventCRFStatus" type="java.lang.String">
            <bind-xml name="OpenClinica:Status" node="attribute"/>
        </field>

        <field name="WorkflowStatusAsString"
               type="java.lang.String">
            <bind-xml name="OpenClinica:WorkflowStatus" node="attribute"/>
        </field>

        <field name="sdvStatusString"
               type="java.lang.String">
            <bind-xml name="OpenClinica:SdvStatus" node="attribute"/>
        </field>

        <field name="ReasonForChangeForCompleteForms" type="java.lang.String">
            <bind-xml name="OpenClinica:ReasonForChangeForCompleteForms" node="attribute"/>
        </field>

        <field name="ItemGroupData"
               type="core.org.akaza.openclinica.bean.submit.crfdata.ImportItemGroupDataBean"
               collection="arraylist">
            <bind-xml name="ItemGroupData"/>
        </field>
    </class>

    <class name="core.org.akaza.openclinica.bean.submit.crfdata.ImportItemGroupDataBean">
        <map-to xml="ItemGroupData"/>
        <field name="ItemGroupOID"
               type="java.lang.String">
            <bind-xml name="ItemGroupOID" node="attribute"/>
        </field>

        <field name="ItemGroupRepeatKey"
               type="java.lang.String">
            <bind-xml name="ItemGroupRepeatKey" node="attribute"/>
        </field>

        <field name="removed"
               type="java.lang.String">
            <bind-xml name="Removed" node="attribute"/>
        </field>

        <field name="ItemData"
               type="core.org.akaza.openclinica.bean.submit.crfdata.ImportItemDataBean"
               collection="arraylist">
            <bind-xml name="ItemData"/>
        </field>
    </class>

    <class name="core.org.akaza.openclinica.bean.submit.crfdata.ImportItemDataBean">
        <map-to xml="ItemData"/>
        <field name="ItemOID"
               type="java.lang.String">
            <bind-xml name="ItemOID" node="attribute"/>
        </field>
        <field name="Value"
               type="java.lang.String">
            <bind-xml name="Value" node="attribute"/>
        </field>
        <field name="TransactionType"
               type="java.lang.String">
            <bind-xml name="TransactionType" node="attribute"/>
        </field>
        <field name="IsNull"
               type="java.lang.String">
            <bind-xml name="IsNull" node="attribute"/>
        </field>
        <field name="discrepancyNotes"
               type="core.org.akaza.openclinica.bean.odmbeans.DiscrepancyNotesBean">
            <bind-xml name="OpenClinica:DiscrepancyNotes"/>
        </field>
    </class>
    <class name="core.org.akaza.openclinica.bean.odmbeans.DiscrepancyNotesBean">
        <map-to xml="OpenClinica:DiscrepancyNotes"/>
        <field name="DiscrepancyNotes"
               type="core.org.akaza.openclinica.bean.odmbeans.DiscrepancyNoteBean"
               collection="arraylist">
            <bind-xml name="OpenClinica:DiscrepancyNote"/>
        </field>
    </class>
    <class name="core.org.akaza.openclinica.bean.odmbeans.DiscrepancyNoteBean">
        <map-to xml="OpenClinica:DiscrepancyNote"/>
        <field name="displayId"
               type="java.lang.String">
            <bind-xml name="ID" node="attribute"/>
        </field>
        <field name="noteType"
               type="java.lang.String">
            <bind-xml name="NoteType" node="attribute"/>
        </field>
        <field name="childNotes"
               type="core.org.akaza.openclinica.bean.odmbeans.ChildNoteBean"
               collection="arraylist">
            <bind-xml name="OpenClinica:ChildNote"/>
        </field>
    </class>
    <class name="core.org.akaza.openclinica.bean.odmbeans.ChildNoteBean">
        <map-to xml="OpenClinica:ChildNote"/>
        <field name="displayId"
               type="java.lang.String">
            <bind-xml name="ID" node="attribute"/>
        </field>
        <field name="status"
               type="java.lang.String">
            <bind-xml name="Status" node="attribute"/>
        </field>
        <field name="ownerUserName"
               type="java.lang.String">
            <bind-xml name="UserName" node="attribute"/>
        </field>
        <field name="DetailedNote"
               type="java.lang.String">
            <bind-xml name="OpenClinica:DetailedNote" node="element"/>
        </field>
        <field name="userRef"
               type="core.org.akaza.openclinica.bean.odmbeans.ElementRefBean">
            <bind-xml name="UserRef"/>
        </field>
    </class>
    <class name="core.org.akaza.openclinica.bean.odmbeans.ElementRefBean">
        <map-to xml="UserRef"/>
        <field name="userName"
               type="java.lang.String">
            <bind-xml name="OpenClinica:UserName" node="attribute"/>
        </field>
    </class>
</mapping>