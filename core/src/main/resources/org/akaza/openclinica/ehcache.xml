<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../properties/ehcache.xsd">
<defaultCache 
	maxElementsInMemory="500000" 
	eternal="false" 
	overflowToDisk="false" 
	timeToIdleSeconds="10" 
	timeToLiveSeconds="20" 
	diskPersistent="false" />
<cache name="com.akaza.openclinica.dao.core.DAOCache" eternal="false"
    maxElementsInMemory="10000" 
    overflowToDisk="false" 
    diskPersistent="false"
    timeToIdleSeconds="0" 
    timeToLiveSeconds="90"
    memoryStoreEvictionPolicy="LFU" />
<cache
	name="org.hibernate.cache.StandardQueryCache"
	maxElementsInMemory="500000"
	eternal="false"
	timeToLiveSeconds="240"
	overflowToDisk="false" diskPersistent="false" memoryStoreEvictionPolicy="LFU"  />
<cache
    name="org.akaza.openclinica.domain.datamap.StudySubject"
    maxElementsInMemory="10000"
    eternal="false"
    timeToIdleSeconds="300"
    timeToLiveSeconds="600" statistics="true"
    overflowToDisk="false" diskPersistent="false" memoryStoreEvictionPolicy="LFU">
</cache>

<cache
	name="org.akaza.openclinica.domain.datamap.StudySubject.studyEvents"
    maxElementsInMemory="450"
    eternal="false"
    timeToLiveSeconds="600" statistics="true"
    overflowToDisk="false" diskPersistent="false" memoryStoreEvictionPolicy="LFU">
</cache>
    
 <!-- Page and Page Fragment Caches -->
<cache name="CachePage1CachingFilter"
  maxElementsInMemory="10"
  eternal="false"
  timeToIdleSeconds="10000"
  timeToLiveSeconds="10000"
  overflowToDisk="false">
</cache>
<cache name="CachedPage2Cache"
  maxElementsInMemory="10"
  eternal="false"
  timeToLiveSeconds="3600"
  overflowToDisk="false">
</cache>
<cache name="SimplePageFragmentCachingFilter"
  maxElementsInMemory="10"
  eternal="false"
  timeToIdleSeconds="10000"
  timeToLiveSeconds="10000"
  overflowToDisk="false">
</cache>
<cache name="SimpleCachingHeadersTimeoutPageCachingFilter"
  maxElementsInMemory="10"
  eternal="false"
  timeToIdleSeconds="10000"
  timeToLiveSeconds="10000"
  overflowToDisk="false">
</cache>

<cache name="randomizeObject"
  maxElementsInMemory="200"
  eternal="false"
  timeToIdleSeconds="10000"
  timeToLiveSeconds="10000"
  overflowToDisk="false">
</cache>

</ehcache>

