<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>

    <class name="org.akaza.openclinica.web.pform.formlist.XFormList">
        <map-to xml="xforms" ns-uri="http://openrosa.org/xforms/xformsList"/>
        
        <field name="XForms"
            type="org.akaza.openclinica.web.pform.formlist.XForm"
            collection="arraylist">
            <bind-xml name="xform"/>
        </field>
    </class>
    
    <class name="org.akaza.openclinica.web.pform.formlist.XForm">
        <map-to xml="xform" ns-uri="http://openrosa.org/xforms/xformsList"/>
       
       <field name="FormID"
               type="java.lang.String">
            <bind-xml name="formID" node="element"/>
       </field>
       <field name="Name"
               type="java.lang.String">
            <bind-xml name="name" node="element"/>
       </field>
       <field name="MajorMinorVersion"
               type="java.lang.String">
            <bind-xml name="majorMinorVersion" node="element"/>
       </field>
       <field name="Version"
               type="java.lang.String">
            <bind-xml name="version" node="element"/>
       </field>
       <field name="Hash"
               type="java.lang.String">
            <bind-xml name="hash" node="element"/>
       </field>
       <field name="DownloadURL"
               type="java.lang.String">
            <bind-xml name="downloadUrl" node="element"/>
       </field>
       <field name="ManifestURL"
               type="java.lang.String">
            <bind-xml name="manifestUrl" node="element"/>
       </field>
    </class>
 </mapping>