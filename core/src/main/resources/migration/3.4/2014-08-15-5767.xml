<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="jkeremian" id="2014-08-15-5767">
        <comment>Insert a new record in the studyParameter table</comment>
        <insert tableName="study_parameter">
            <column name="study_parameter_id" valueNumeric="17"/>
            <column name="handle" value="participantPortal"/>
            <column name="name" value="participantPortal"/>
            <column name="description" value="Participant Portal Enabled or Diabled option"/>
            <column name="default_value" value="disabled"/>
            <column name="inheritable" valueBoolean="true"/>
            <column name="overridable" valueBoolean="false"/>
        </insert>
    </changeSet>
</databaseChangeLog>